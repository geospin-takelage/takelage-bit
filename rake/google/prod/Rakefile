# frozen_string_literal: true

require 'rake'

namespace :google do
  desc 'Create and deploy image to google'
  task prod: %w[git:warn
                rubylint
                test:project
                image:prod:build
                image:prod:test
                git:tag
                google:prod:registry:tag
                google:prod:registry:push
                google:prod:container:update]
end
